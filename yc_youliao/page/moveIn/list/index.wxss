page{
	background-color: #f4f4f4;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	font-family: PingFangSC-Regular;
}
.moveInList{
	width:100%;
	box-sizing:border-box;
	background:#fff;
	margin-top:20rpx;
}
.home-list .home-list-item{
	padding: 15rpx 20rpx; 
	display: flex;
	border-bottom: 0.5rpx solid #dedede;
	position:relative;
}
.home-list .home-list-item .pic{
	width: 140rpx;
	height: 140rpx;
}
.home-list .home-list-item .pic .img{
	width: 100%;
	height: 100%;
}
.home-list .home-list-item .desc{
	flex: 1;
	margin-left: 15rpx;
}
.home-list-item .desc .title{
	display: flex;
	justify-content: space-between;
	font-size:30rpx;
}
.home-list-item .desc .title .name{
	font-weight:600;
}
.home-list-item .desc .tabs{
	display: flex;
}
.home-list-item .desc .tabs .tab-itam{
	font-size:23rpx;
	padding:0 5rpx;
	border:0.5rpx solid #FA8072;
	border-radius:10rpx;
	height:36rpx;
	line-height:36rpx;
	padding-bottom:;
	padding:0 8rpx;
	margin-right:20rpx;
	color: #FA8072;
}
.score-box{
	text-align: left;
	line-height: 26rpx;
	margin: 10rpx 0 20rpx 0;
	display: flex;
	height: 26rpx;
}
.score-list{
	height: 26rpx;
	line-height: 26rpx;
	display: flex;
}
.score-text{
	font-size: 23rpx;
	color: #FF8C00;
	margin-top:7rpx;
	margin-left: 4rpx;
}
.score-box .iconfont{
	color: #FF8C00;
	font-size: 23rpx;
}
.btn-box{
	position: absolute;
	right: 0;
	top: 0;
	height: 40rpx;
	line-height: 40rpx;
	width: 95rpx;
	text-align: center;
	color: #fff;
	background: rgba(0,0,0,0.6);
	font-size: 24rpx;
}
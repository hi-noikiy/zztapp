page{
	background-color: #f4f4f4;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	font-family: PingFangSC-Regular;
}
.containner-movein-create{
	width: 100%;
	font-size: 24rpx;
}
.header{
	color: #fff;
	text-align: center;
	background: #FFA500;
	padding:42rpx 0;
	line-height:41rpx;
}
.logo-box{
	display: flex;
	border-bottom: 0.5rpx solid #dedede;
	background: #fff;
}
.logo-box .pic{
	width: 120rpx;
	height: 120rpx;
	background: #dedede;
	margin: 20rpx 30rpx 0 20rpx;
}
.pic-text{
	text-align:center;
	width:68rpx;
	height:50rpx;
	margin:30rpx auto 0;
	color:#979797;
}
.logo-box-right{
	flex: 1;
}
.item{
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	position: relative;
}
.item-shop-name{
	border-bottom: 0.5rpx solid #dedede;
}
.item input{
	height: 100%;
}
.item .btn{
	position: absolute;
	right: 20rpx;
	top: 50%;
	transform: translateY(-50%);
	height: 40rpx;
	line-height: 40rpx;
	text-align: center;
	border: 0.5rpx solid #dedede;
	color: #979797;
	border-radius: 15rpx;
	width: 100rpx;
}
.identify, .shop, .time-box, .desc-box, .shop-pic, .classify{
	display: flex;
	line-height: 80rpx;
	height: 80rpx;
	background: #fff;
	padding-left: 20rpx;
}
.adress-box{
	background: #fff;
	text-align: center;
	padding-top: 20rpx;
}

.msg{
	font-size: 20rpx;
	color: #979797;
	margin: 20rpx 0 10rpx 0;
}
.btn-box{
	display:flex;
	width:36%;
	margin:0 auto;
}
.btn-left, .btn-right{
	height: 40rpx;
	line-height: 40rpx;
	width: 130rpx;
	text-align: center;
	border-radius: 10rpx;
	border: 0.5rpx solid #dedede;
	color: #979797;
}
.label{
	width: 150rpx;
	height: 100%;
}
.identify{
	margin-bottom: 20rpx;
}
.identify input{
	height: 100%;
	flex: 1;
}
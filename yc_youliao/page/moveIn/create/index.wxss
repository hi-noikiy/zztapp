page{
	background-color: #f4f4f4;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	font-family: PingFangSC-Regular;
}
.containner-movein-create{
	width: 100%;
	font-size: 26rpx;
}
.header{
	color: #fff;
	text-align: center;
	background: #FFA500;
	position: relative;
	height:250rpx;
}
.banner-img{
	width: 100%;
	height: 250rpx;
}
.header .title , .header .info{
	position: absolute;
	left: 50%;
	width:100%;
	font-size: 28rpx;
	transform: translateX(-50%);
}
.header .title{
	top: 30%;
}
.header .info{
	top: 50%;
}
.logo-box{
	display: flex;
	background: #fff;
	margin-bottom: 15rpx;
}
.logo-box .pic{
	width: 130rpx;
	height: 130rpx;
	background: #dedede;
	margin: 20rpx 30rpx 0 20rpx;
}
.logo-img{
	height: 100%;
	width: 100%;
}

.pic-text{
	text-align:center;
	width:68rpx;
	height:50rpx;
	margin:30rpx auto 0;
	color:#777;
}
.logo-box-right{
	flex: 1;
}
.item{
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	position: relative;
}
.item-shop-name{
	border-bottom: 0.5rpx solid #dedede;
}
.item input{
	height: 100%;
}
.item .btn{
	position: absolute;
	right: 20rpx;
	top: 50%;
	transform: translateY(-50%);
	height: 50rpx;
	line-height: 50rpx;
	text-align: center;
	border: 0.5rpx solid #dedede;
	color: #979797;
	border-radius: 15rpx;
	width: 100rpx;
}
.identify, .shop, .time-box, .desc-box, .shop-pic, .classify, .time-box-right{
	display: flex;
	line-height: 80rpx;
	background: #fff;
	padding-left: 20rpx;
}
.time-box-right{
	padding: 0;
}
.time-box-right .picker-left{
	margin-right: 30rpx;
}
.time-box-right .picker-right{
	margin-left: 30rpx;
}

.identify, .time-box, .classify{
	height: 80rpx;
}
.desc-box{
	position: relative;
}
.desc-box, .shop{
	margin-top: 15rpx;
}
.desc-box textarea{
	height: 220rpx;
}
.placeholder{
	position: absolute;
	left:167rpx;
	top:-1rpx;
	color:#777;
}
.shop{
	padding:10rpx 0 10rpx 20rpx;
	border-bottom: 0.5rpx solid #dedede;
	box-sizing: border-box;
}
.adress-box{
	background: #fff;
	text-align: center;
	padding-top: 20rpx;
	padding-bottom: 20rpx;
}
.adress-box .title{
	font-size: 28rpx;
}
.time-box input{
	height: 100%;
	width:100%;
	padding-left:25rpx;
	box-sizing:border-box;
}
.msg{
	font-size: 25rpx;
	color: #979797;
	margin: 15rpx 0;
}
.checkbox{
	width:50%;
	display:inline-block;
}
.btn-box{
	display:flex;
	width:36%;
	margin:0 auto;
}
.btn-left, .btn-right{
	height: 45rpx;
	line-height: 45rpx;
	width: 130rpx;
	text-align: center;
	border-radius: 10rpx;
	border: 0.5rpx solid #dedede;
	color: #979797;
}
.btn-left{
	color: #FFA500;
	margin-right: 45rpx;
	border: 0.5rpx solid #FFA500;
}
.label{
	width: 150rpx;
	height: 100%;
	font-size: 28rpx;
}
.identify{
	margin-bottom: 20rpx;
}
.identify input{
	height: 100%;
	flex: 1;
}
.shop-pic{
	margin: 15rpx 0;
	padding-bottom: 20rpx;
}
.shop-pic .text{
	color: #979797;
}
.text-active{
	color: #333;
}
.pic-box{
	display: flex;
	flex-wrap: wrap;
}
.plus{
	border: 0.5rpx solid #dedede;
	text-align: center;
}
.plus .iconfont{
	color: #979797;
	font-size: 80rpx;
}
.pic-upload{
	flex: 1;
}
.pic-item{
	width: 160rpx;
	height: 160rpx;
	line-height:160rpx;
	margin-right: 20rpx;
	margin-bottom: 20rpx;
	position: relative;
	overflow: visible;
}
.pic-item .icon-del{
	position:absolute;
	right: -19rpx;
	top: -80rpx;
	font-size: 30rpx;
	color: #999;
}
.classify{
	color: #979797;
	position: relative;
}
.classify .text{
	margin-left: -2rpx;
}
.classify .iconfont{
	position: absolute;
	right: 20rpx;
	top: 50%;
	transform: translateY(-50%);
	font-size: 29rpx;
	color: #777;
}
.classify .label{
	color: #000;
}
.footer-btn{
	width:94%;
	height:75rpx;
	line-height:75rpx;
	text-align:center;
	background:#ff7803;
	border-radius:10rpx;
	color:#fff;
	margin:80rpx auto 60rpx;
}
.caution{
	padding:20rpx;
	color:#979797;
	font-size:22rpx;
}
.caution i{
	color: #000080;
}
.textarea-placeholder{
	margin-top: 27rpx;
}
page{
	background-color: #fff;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	font-family: PingFangSC-Regular;
}
.container-load{
	width: 100%;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #333;
	text-align: center;
}
.title{
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 30rpx;
	color: #979797;
	border-bottom: 1rpx solid #dedede;
}
.list{
	width: 100%;
	display: flex;
	flex-wrap: wrap;
	padding: 0 30rpx;
	box-sizing: border-box;
}
.item-box{
	width: 25%;
	height: 205rpx;
}
.pic{
	width: 90rpx;
	height: 90rpx;
	border-radius: 50%;
	background: #dedede;
	disply: block;
	margin: 36rpx auto 20rpx;
}
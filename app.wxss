.clearfix:after, .clearfix:before {
  content: "";
  display: table;
}

.clearfix:after {
  clear: both;
}

.newItem {
  padding: 30rpx 20rpx 16rpx;
  margin-bottom: 10rpx;
}

.newItem .top {
  line-height: 30px;
  font-size: 28rpx;
  margin-bottom: 16rpx;
  display: flex;
  position: relative;
}
.newItem .top .wechat{
  color: #fb5100;
  position: absolute;
  width: 140rpx;
  height: 60rpx;
  line-height: 60rp;
  right: 0;
  bottom: 0;
  border: 1px solid #fb5100;
  border-radius: 4px;
  text-align: center;
  font-size: 26rpx;
  vertical-align: top;
}
.newItem .top .wechat .img{
  display: inline-block;
  vertical-align: top;
  width: 40rpx;
  height: 40rpx;
  padding-top: 10rpx;
  padding-right: 5rpx;
  z-index: 10;
}
.newItem .contentTxt{
  font-size: 32rpx;
  line-height: 50rpx;
}
.newItem .top .avatar{
  flex: 0 0 42px;
  height: 42px;
  overflow: hidden;
  border-radius: 50%;
}
.newItem .top .topRight{
  flex: 1;
  line-height: 24px;
  padding-left: 20rpx;
}
.newItem .top .topRight .txt1{
  font-size: 28rpx;
}
.newItem .top .topRight .txt2{
  font-size: 24rpx;
  line-height: 46rpx;
}
.newItem .top .topRight .ding{
  margin-left: 20rpx;
  font-size: 24rpx;
  padding: 0 16rpx;
  border-radius: 4px;
  border: 1rpx solid #fb5100;
  color: #fb5100;
}
.newItem .top .type {
  margin-left: 0;
  padding: 10rpx 16rpx;
  background-color: #b884c1;
  color: #fff;
  border-radius: 10rpx;
}
.newItem .address {
  font-size: 24rpx;
  color: #999;
  line-height: 60rpx;
  padding-top: 20rpx;
}
.newItem .address text{
  display: inline-block;
  line-height:60rpx;
  padding-top:20rpx;
}
.newItem .address .img{
  width: 30rpx;
  height: 30rpx;
  vertical-align: top;
  padding-top: 15rpx;
}
.newItem .info {
  margin-top: 16rpx;
  height: 50rpx;
  line-height: 66rpx;
  font-size: 26rpx;
  padding-left: 16rpx;
  color: #333;
  border-top: 1rpx solid #eee;
  padding: 10rpx 0;
}
.newItem .info .module{
  color: #fb5100;
  line-height: 66rpx;
  float: left;
}
.newItem text {
  font-size: 24rpx;
}
.newItem .info .img {
  padding-top: 16rpx;
  padding-right: 6rpx;
  width: 30rpx;
  height: 30rpx;
  vertical-align: top;
}

.newItem .images {
  padding-top: 30rpx;
}

.newItem .images image {
  width: 200rpx;
  height: 200rpx;
  float: left;
  padding-right: 20rpx;
}
.apply .hourseTitle{
  font-size: 18px;
  text-align: center;
  padding-top: 20rpx;
  margin-bottom: 20rpx;
}
.apply .item{
  line-height: 80rpx;
  font-size: 30rpx;
  background-color: #fff;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
}
.apply .address{
  color: #97a6d1;
  position: relative;
}
.apply .address:after{
  position: absolute;
  top: 50%;
  right: 15px;
  margin-top: -3px;
  width: 5px;
  height: 5px;
  border: #666 solid;
  border-width: 1rpx 1rpx 0 0;
  content: ' ';
  transform: rotate(45deg)
}
.apply .address image{
  padding: 25rpx 10rpx 0;
  width: 30rpx;
  height: 30rpx;
  vertical-align: top;
}
.apply .smallTitle{
  font-size: 28rpx;
  padding: 28rpx 0 20rpx 28rpx;
}
.apply .item.selectBox{
  padding-top: 20rpx;
  line-height: 60rpx;
}
.apply .item .select{
  display: inline-block;
  border: 1px solid #ddd;
  padding: 0 20rpx;
  border-radius: 4px;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
}
.apply .item .select.checked{
  color: #1aad19;
  border-color: #1aad19;
}
.apply .item .rent:last-child{
  border-bottom: none;
}
.apply .item .icon{
  margin-top:20rpx;
  margin-right:20rpx;
  float: right;
  width:40rpx;
  height:40rpx;
}
.apply .item .title{
  display: inline-block;
}
.apply .item .title.text{
  width: 25%;
}
.apply .item input{
  width: 75%;
  display: inline-block;
  height: 80rpx;
  line-height:80rpx;
  font-size:30rpx;
  vertical-align: top;
}
.apply .item .area{
  padding-top: 10rpx;
  height: 240rpx;
  line-height:80rpx;
  font-size:30rpx;
  vertical-align: top;
}
.apply .item .emptyCircle{
  margin-top:20rpx;
  margin-right:20rpx;
  float:right;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  border: 2px solid #ccc;
  box-sizing: border-box;
}
.applySubmit{
  width: 60%;
  margin: 0 auto;
  background-color: #1aad19;
  text-align: center;
  line-height: 90rpx;
  color: #fff;
  border-radius: 4px;
  margin-bottom: 20rpx;
}
.apply .item.pickerWrapper{
  display: flex;
}
.apply .picker{
  flex: 1;
  color: #666;
}
.emptyData{
  padding-top: 20rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  text-align: center;
}
.titleWrapper{
  display: flex;
  padding: 20rpx 50rpx;
}
.titleWrapper .line{
  flex: 1;
  border-bottom: 1rpx solid #ccc;
  transform: translateY(-28rpx);
}
.titleWrapper .text{
  font-size: 30rpx;
  color: #666;
  line-height: 60rpx;
  padding: 0 20rpx;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 